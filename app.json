{
  "expo": {
    "name": "calibr.ai",
    "slug": "calibrai",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icons/colibrai_logo.png",
    "userInterfaceStyle": "dark",
    "scheme": "calibrai",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/colibrai_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a0f"
    },
    "ios": {
      "supportsTablet": false,
      "icon": "./assets/icons/colibrai_logo.png",
      "bundleIdentifier": "com.guillaumebdx.calibrai",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/colibrai_logo.png",
        "backgroundColor": "#0a0a0f"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.guillaumebdx.calibrai"
    },
    "web": {
      "favicon": "./assets/icons/colibrai_logo.png"
    },
    "plugins": [
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "337bf7b3-35b9-4180-998a-3571755698f2"
      }
    }
  }
}
